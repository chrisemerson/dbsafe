{
    "name": "cemerson/pdosafe",
    "description": "Library for using AWS Parameter Store or AWS Secrets Manager with PDO",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "ext-PDO": "^7.1",
        "aws/aws-sdk-php": "^3.145",
        "psr/cache": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CEmerson\\PDOSafe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CEmerson\\PDOSafe\\": "test"
        }
    },
    "config": {
        "sort-packages": true
    }
}
